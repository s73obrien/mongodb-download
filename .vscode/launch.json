  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\test\\index.ts",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "env": {
        "HTTP_PROXY": "http://10.7.1.236:8080"
      },
      "console": "integratedTerminal",
      "outFiles": ["${workspaceFolder}\\built\\**\\*"],
      "smartStep": true,
      "skipFiles": [
        "${workspaceFolder}\\node_modules\\**\\*"
      ]
    }
  ]
}